{
  "name": "kata-setup-javascript",
  "version": "1.0.0",
  "description": "Basic boilerplate project with Typecript",
  "main": "src/infrastructure/API/main.js",
  "type": "module",
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.5",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3",
    "vitest": "^0.34.5"
  },
  "scripts": {
    "test": "npm run test:unit",
    "test:all": "vitest",
    "test:integration": "node --env-file=.env node_modules/.bin/vitest --run --config vite.config.integration.js",
    "test:unit": "vitest --run --config vite.config.unit.js",
    "test:coverage": "vitest --coverage",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint .",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate": "node src/infrastructure/Shared/migrations/run.js",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgileCraftsmanshipCanarias/kata-setup-javascript.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgileCraftsmanshipCanarias/kata-setup-javascript/issues"
  },
  "homepage": "https://github.com/AgileCraftsmanshipCanarias/kata-setup-javascript#readme",
  "dependencies": {
    "@koa/router": "^12.0.0",
    "dotenv": "^16.3.1",
    "express": "^5.0.0-beta.1",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "mongodb": "^6.1.0",
    "pg": "^8.11.3",
    "zod": "^3.22.2"
  }
}
